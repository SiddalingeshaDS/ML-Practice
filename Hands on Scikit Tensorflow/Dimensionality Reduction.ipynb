   "source": [
    "# numpy svd function to obtain all principal components\n",
    "import numpy as np\n",
    "\n",
    "def get_first_two_pc(X):\n",
    "    X_centered = X - X.mean(axis=0)\n",
    "    U, s, Vt = np.linalg.svd(X_centered)\n",
    "    c1 = Vt.T[:, 0]\n",
    "    c2 = Vt.T[:, 1]\n",
    "    return (c1, c2)"
   ]
  }
